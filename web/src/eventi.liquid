---
layout: layouts/base.liquid
title: Eventi subnettuno.it
linkHome: true
---

<div class="column">
    <div class="grid grid-cols-2 gap-4 lg:grid-cols-3">
    {% for evento in eventi %}
    <article class="relative">
        <div className="mb-6 py-2">
            <h2>{{ evento.title }} </h2>
        </div>
        <div>
           <img src="{{evento.locandina | sanityImage}}" alt="{{evento.alt}}" class="max-h-[600px] w-full object-contain">
        </div>
    </article>
    {% endfor %}
    </div>
</div>